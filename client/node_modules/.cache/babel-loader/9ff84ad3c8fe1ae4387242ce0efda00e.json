{"ast":null,"code":"import UserList from '../components/AdminConsole/UserList.vue';\n\nconst axios = require('axios').default;\n\nconst USER_INFO_URL = 'http://localhost:4000/getActivityList';\nexport default {\n  components: {\n    'user-list': UserList\n  },\n\n  data() {\n    return {\n      USER_DATA: []\n    };\n  },\n\n  provide() {\n    return {\n      userData: this.USER_DATA\n    };\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch('setIsLogOut');\n      this.$store.dispatch('setIsUser', {\n        isUser: null\n      });\n      this.$router.push('/');\n    },\n\n    /**\n     * @description\n     * Load all the user data from database, data is stored in \n     */\n    loadUserData() {\n      axios.post(USER_INFO_URL).then(response => {\n        const DATA_INDEX = 0;\n\n        for (let i = 0; i < response.data[DATA_INDEX].length; i += 1) {\n          this.USER_DATA.push(response.data[DATA_INDEX][i]);\n        }\n      });\n    }\n\n  },\n\n  mounted() {\n    this.loadUserData();\n  }\n\n};","map":{"version":3,"mappings":"AASA,OAAOA,QAAP,MAAqB,yCAArB;;AAEA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,MAAMC,aAAY,GAAI,uCAAtB;AAEA,eAAe;AACbC,YAAU,EAAE;AACV,iBAAaL;AADH,GADC;;AAIbM,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE;AADN,KAAP;AAGD,GARY;;AASbC,SAAO,GAAG;AACR,WAAO;AACLC,cAAQ,EAAE,KAAKF;AADV,KAAP;AAGD,GAbY;;AAcbG,SAAO,EAAE;AACPC,UAAM,GAAG;AACP,WAAKC,MAAL,CAAYC,QAAZ,CAAqB,aAArB;AACA,WAAKD,MAAL,CAAYC,QAAZ,CAAqB,WAArB,EAAkC;AAAEC,cAAM,EAAE;AAAV,OAAlC;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB,GAAlB;AACD,KALM;;AAMP;;;;AAIAC,gBAAY,GAAG;AACbhB,WAAI,CACDiB,IADH,CACQd,aADR,EAEGe,IAFH,CAESC,QAAD,IAAc;AAClB,cAAMC,UAAS,GAAI,CAAnB;;AACA,aAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAIF,QAAQ,CAACd,IAAT,CAAce,UAAd,EAA0BE,MAA9C,EAAsDD,CAAC,IAAE,CAAzD,EAA4D;AAC1D,eAAKf,SAAL,CAAeS,IAAf,CAAoBI,QAAQ,CAACd,IAAT,CAAce,UAAd,EAA0BC,CAA1B,CAApB;AACF;AACD,OAPH;AAQD;;AAnBM,GAdI;;AAmCbE,SAAO,GAAG;AACR,SAAKP,YAAL;AACD;;AArCY,CAAf","names":["UserList","axios","require","default","USER_INFO_U