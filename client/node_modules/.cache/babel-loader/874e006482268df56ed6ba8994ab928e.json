{"ast":null,"code":"const axios = require('axios').default;\n\nconst GETEMAIL_URL = 'http://localhost:4000/getEmail';\nconst GETCOUNT_URL = 'http://localhost:4000/getCount';\nconst REGISTER_URL = 'http://localhost:4000/register';\nexport default {\n  name: 'AboutView',\n\n  data() {\n    return {\n      email: null,\n      count: null,\n      message: null\n    };\n  },\n\n  mounted() {\n    axios.get(GETEMAIL_URL).then(results => {\n      this.email = results.data.email_address;\n    });\n    axios.get(GETCOUNT_URL).then(results => {\n      this.count = results.data.numberOfRegistery;\n    });\n  },\n\n  methods: {\n    postMessage() {\n      const content = this.message;\n      console.log(content);\n      axios.post(REGISTER_URL, {\n        message: content\n      });\n      this.$forceUpdate();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,MAAMC,YAAW,GAAI,gCAArB;AACA,MAAMC,YAAW,GAAI,gCAArB;AACA,MAAMC,YAAW,GAAI,gCAArB;AAEA,eAAe;AACbC,MAAI,EAAE,WADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,IADF;AAELC,WAAK,EAAE,IAFF;AAGLC,aAAO,EAAE;AAHJ,KAAP;AAKD,GATY;;AAWbC,SAAO,GAAG;AACRX,SAAK,CAACY,GAAN,CAAUT,YAAV,EACGU,IADH,CACSC,OAAD,IAAa;AACjB,WAAKN,KAAL,GAAaM,OAAO,CAACP,IAAR,CAAaQ,aAA1B;AACD,KAHH;AAKAf,SAAK,CAACY,GAAN,CAAUR,YAAV,EACGS,IADH,CACSC,OAAD,IAAa;AACjB,WAAKL,KAAL,GAAaK,OAAO,CAACP,IAAR,CAAaS,iBAA1B;AACD,KAHH;AAID,GArBY;;AAuBbC,SAAO,EAAE;AACPC,eAAW,GAAG;AACZ,YAAMC,OAAM,GAAI,KAAKT,OAArB;AACAU,aAAO,CAACC,GAAR,CAAYF,OAAZ;AACAnB,WAAK,CAACsB,IAAN,CAAWjB,YAAX,EAAyB;AAAEK,eAAO,EAAES;AAAX,OAAzB;AACA,WAAKI,YAAL;AACD;;AANM;AAvBI,CAAf","names":["axios","require","default","GETEMAIL_URL","GETCOUNT_URL","REGISTER_URL","name","data","email","count","message","mounted","get","then","results","email_address","numberOfRegistery","methods","postMessage","content","console","log","post","$forceUpdate"],"sourceRoot":"","sources":["D:\\MySQL\\join_us_vue\\client\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"about\">\n    <h1>This is an about page</h1>\n  </div>\n\n  <div class=\"show latest email\">\n    <h1>This is the latest received email</h1>\n    <h1>{{ email }}</h1>\n    <h1> The total number of email registered is {{ count }} </h1>\n  </div>\n\n  <div class=\"userInput\">\n    <input v-model=\"message\" placeholder=\"edit me\">\n    <button v-on:click=\"postMessage\">Register</button>\n    <p>Message is: {{ message }}</p>\n  </div>\n\n</template>\n\n<script>\nconst axios = require('axios').default;\n\nconst GETEMAIL_URL = 'http://localhost:4000/getEmail';\nconst GETCOUNT_URL = 'http://localhost:4000/getCount';\nconst REGISTER_URL = 'http://localhost:4000/register';\n\nexport default {\n  name: 'AboutView',\n\n  data() {\n    return {\n      email: null,\n      count: null,\n      message: null,\n    };\n  },\n\n  mounted() {\n    axios.get(GETEMAIL_URL)\n      .then((results) => {\n        this.email = results.data.email_address;\n      });\n\n    axios.get(GETCOUNT_URL)\n      .then((results) => {\n        this.count = results.data.numberOfRegistery;\n      });\n  },\n\n  methods: {\n    postMessage() {\n      const content = this.message;\n      console.log(content);\n      axios.post(REGISTER_URL, { message: content });\n      this.$forceUpdate();\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}