{"ast":null,"code":"const axios = require('axios').default;\n\nconst GETEMAIL_URL = 'http://localhost:4000/getEmail';\nconst REGISTER_URL = 'http://localhost:4000/register';\nexport default {\n  name: 'AboutView',\n\n  data() {\n    return {\n      email: null,\n      message: null\n    };\n  },\n\n  mounted() {\n    axios.get(GETEMAIL_URL).then(results => {\n      this.email = results.data.email_address;\n    });\n  },\n\n  methods: {\n    postMessage: function () {\n      var content = this.message;\n      axios.post(REGISTER_URL, {\n        content\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAmBA,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,MAAMC,YAAW,GAAI,gCAArB;AACA,MAAMC,YAAW,GAAI,gCAArB;AAEA,eAAe;AACbC,MAAI,EAAE,WADO;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,IADF;AAELC,aAAO,EAAE;AAFJ,KAAP;AAID,GARY;;AAUbC,SAAO,GAAG;AACRT,SAAK,CAACU,GAAN,CAAUP,YAAV,EACGQ,IADH,CACSC,OAAD,IAAa;AACjB,WAAKL,KAAL,GAAaK,OAAO,CAACN,IAAR,CAAaO,aAA1B;AACD,KAHH;AAID,GAfY;;AAiBbC,SAAO,EAAE;AACPC,eAAW,EAAE,YAAY;AACvB,UAAIC,OAAM,GAAI,KAAKR,OAAnB;AACAR,WAAK,CAACiB,IAAN,CAAWb,YAAX,EAAyB;AAAEY;AAAF,OAAzB;AACF;AAJO;AAjBI,CAAf","names":["axios","require","default","GETEMAIL_URL","REGISTER_URL","name","data","email","message","mounted","get","then","results","email_address","methods","postMessage","content","post"],"sourceRoot":"","sources":["D:\\MySQL\\join_us_vue\\client\\src\\views\\AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"about\">\n    <h1>This is an about page</h1>\n  </div>\n\n  <div class=\"show latest email\">\n    <h1>This is the latest received email</h1>\n    <h1>{{ email }}</h1>\n  </div>\n\n  <div class=\"userInput\">\n    <input v-model=\"message\" placeholder=\"edit me\">\n    <button v-on:click=\"postMessage\">Register</button>\n    <p>Message is: {{ message }}</p>\n  </div>\n\n</template>\n\n<script>\nconst axios = require('axios').default;\n\nconst GETEMAIL_URL = 'http://localhost:4000/getEmail';\nconst REGISTER_URL = 'http://localhost:4000/register';\n\nexport default {\n  name: 'AboutView',\n\n  data() {\n    return {\n      email: null,\n      message: null,\n    };\n  },\n\n  mounted() {\n    axios.get(GETEMAIL_URL)\n      .then((results) => {\n        this.email = results.data.email_address;\n      });\n  },\n\n  methods: {\n    postMessage: function () {\n      var content = this.message;\n      axios.post(REGISTER_URL, { content })\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}