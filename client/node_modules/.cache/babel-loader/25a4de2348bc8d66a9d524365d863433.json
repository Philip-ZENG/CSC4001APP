{"ast":null,"code":"import { Swiper, SwiperSlide } from 'vue-awesome-swiper';\nimport Datepicker from 'vuejs3-datepicker';\nimport ActivityCard from '../components/ActivityCard.vue';\n\nconst axios = require('axios').default;\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n    Datepicker,\n    'activity-card': ActivityCard\n  },\n  computed: {\n    totalActivityNum() {\n      return this.actInformation.length;\n    }\n\n  },\n\n  data() {\n    return {\n      selected: '',\n      actInformation: []\n    };\n  },\n\n  methods: {\n    switchTo(path) {\n      this.$router.replace(path);\n    },\n\n    askInfo() {\n      axios.post('http://localhost:4000/getActivityInfo').then(response => {\n        this.actInformation = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.askInfo();\n  }\n\n};","map":{"version":3,"mappings":"AAsDA,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,oBAApC;AACA,OAAOC,UAAP,MAAuB,mBAAvB;AACA,OAAOC,YAAP,MAAyB,gCAAzB;;AAEA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,eAAe;AACbC,YAAU,EAAE;AACVP,UADU;AAEVC,eAFU;AAGVC,cAHU;AAIV,qBAAiBC;AAJP,GADC;AAQbK,UAAQ,EAAE;AACRC,oBAAgB,GAAG;AACjB,aAAO,KAAKC,cAAL,CAAoBC,MAA3B;AACD;;AAHO,GARG;;AAcbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELH,oBAAc,EAAE;AAFX,KAAP;AAID,GAnBY;;AAqBbI,SAAO,EAAE;AACPC,YAAQ,CAACC,IAAD,EAAO;AACb,WAAKC,OAAL,CAAaC,OAAb,CAAqBF,IAArB;AACD,KAHM;;AAKPG,WAAO,GAAG;AACRf,WAAK,CAACgB,IAAN,CAAW,uCAAX,EACGC,IADH,CACSC,QAAD,IAAc;AAClB,aAAKZ,cAAL,GAAsBY,QAAQ,CAACV,IAA/B;AACD,OAHH,EAIGW,KAJH,CAIUC,KAAD,IAAW;AAChBC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OANH;AAOD;;AAbM,GArBI;;AAqCbG,SAAO,GAAG;AACR,SAAKR,OAAL;AACD;;AAvCY,CAAf","names":["Swiper","SwiperSlide","Datepicker","ActivityCard","axios","require","default","components","computed","totalActivityNum","actInformation","length","data","selected","methods","switchTo","path","$router","replace","askInfo","post","then","response","catch","error","console","log","mounted"],"sourceRoot":"","sources":["C:\\Users\\22414\\Desktop\\App\\CSC4001APP\\client\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <table class=\"showTable\" cellspacing=\"0\" cellpadding=\"0\" align=\"center\">\n      <tr height=\"100\">\n        <td id=\"swip\" align=\"center\" colspan=\"3\" border=\"1\">\n          <swiper options=\"swiperOption\" class = 'swiper-box'>\n            <swiper-slide height=\"50\" align=\"center\">\n              <div >content</div>\n            </swiper-slide>\n          </swiper>\n        </td>\n        <td id=\"create\" align=\"center\">\n          <button id = \"createNew\" @click=\"switchTo('/activityCreation')\">\n          + Post a new event</button>\n        </td>\n      </tr>\n      <tr height=\"50\">\n        <td align=\"center\" width=\"30%\">search bar</td>\n        <td align=\"center\" width=\"20%\">\n          <div class=\"rightSep\" id=\"order-select\">\n            <select v-model=\"selected\">\n            <option disabled value=\"\">Please select one</option>\n            <option>A</option>\n            <option>B</option>\n            <option>C</option>\n          </select>\n          <span>Selected: {{ selected }}</span>\n          </div>\n        </td>\n        <td align=\"center\" width=\"20%\">\n          <div class=\"rightSep\">\n            <datepicker>\n            </datepicker>\n          </div>\n        </td>\n        <td align=\"center\" width=\"30%\">number of member</td>\n      </tr>\n    </table>\n\n    <div class=\"actSquare\">\n      <dl>\n        <dt v-for=\"(act, index) in actInformation\" :key=\"index\">\n          <activity-card :time=\"act.time\" :title=\"act.title\"> </activity-card>\n        </dt>\n      </dl>\n      <div class=\"pageList\">\n        <div class=\"pageIndex\">1</div>\n        <div class=\"pageIndex\">{{totalActivityNum}}</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Swiper, SwiperSlide } from 'vue-awesome-swiper';\nimport Datepicker from 'vuejs3-datepicker';\nimport ActivityCard from '../components/ActivityCard.vue';\n\nconst axios = require('axios').default;\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n    Datepicker,\n    'activity-card': ActivityCard,\n  },\n\n  computed: {\n    totalActivityNum() {\n      return this.actInformation.length;\n    },\n  },\n\n  data() {\n    return {\n      selected: '',\n      actInformation: [],\n    };\n  },\n\n  methods: {\n    switchTo(path) {\n      this.$router.replace(path);\n    },\n\n    askInfo() {\n      axios.post('http://localhost:4000/getActivityInfo')\n        .then((response) => {\n          this.actInformation = response.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n\n  mounted() {\n    this.askInfo();\n  },\n\n};\n</script>\n\n<style>\n#swip {\n  border-style: solid;\n  border-width: 0px 1px 1px 0px;\n}\n\n#create {\n  border-style: solid;\n  border-width: 0px 0px 1px 0px;\n}\n\n#createNew {\n  text-align:center;\n  background-color: white;\n  border-radius: 10px;\n  height:45px;\n  width: 200px;\n\n}\n\n.rightSep{\n  margin: 5px;\n  border-style: solid;\n  border-width: 0px 1px 0px 0px;\n}\n\n.showTable{\n  width: 100%;\n  border-style: solid;\n  border-width: 1px 0px 1px 0px;\n  border-color: black;\n}\n\n.actSquare{\n  padding-left: 4%;\n  padding-right: 4%;\n  padding-top: 1%;\n  align-content: center;\n}\n\n.pageList{\n  position: fixed;\n  bottom: 0%;\n  width: 100%;\n  height: 5%;\n  background-color: white;\n}\n\n.pageIndex{\n  width: 30px;\n  height: 30px;\n  border-style: solid;\n  border-width: 1px;\n  border-radius: 5px;\n  text-align: center;\n  color: slateblue;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module"}