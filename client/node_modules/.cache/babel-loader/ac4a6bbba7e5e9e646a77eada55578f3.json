{"ast":null,"code":"import { Swiper, SwiperSlide } from 'vue-awesome-swiper';\nimport Datepicker from 'vuejs3-datepicker';\nimport ActivityCard from '../components/ActivityCard.vue';\n\nconst axios = require('axios').default;\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide,\n    Datepicker,\n    'activity-card': ActivityCard\n  },\n\n  data() {\n    return {\n      selected: '',\n      swiperOption: {\n        autoplay: 3000,\n        pagination: '.swiper-pagination',\n        autoplayDisableOnInteraction: true,\n        loop: true\n      },\n      swipers: [],\n      actInfos: []\n    };\n  },\n\n  methods: {\n    switchTo(path) {\n      this.$router.replace(path);\n    },\n\n    askInfo() {\n      axios.post('http://localhost:4000/getActivityInfo').then(function (response) {\n        console.log(response);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.askInfo();\n  }\n\n};","map":{"version":3,"mappings":"AAiDA,SAASA,MAAT,EAAiBC,WAAjB,QAAoC,oBAApC;AACA,OAAOC,UAAP,MAAuB,mBAAvB;AACA,OAAOC,YAAP,MAAyB,gCAAzB;;AAEA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,eAAe;AACbC,YAAU,EAAE;AACVP,UADU;AAEVC,eAFU;AAGVC,cAHU;AAIV,qBAAiBC;AAJP,GADC;;AAQbK,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,kBAAY,EAAE;AACZC,gBAAQ,EAAE,IADE;AAEZC,kBAAU,EAAE,oBAFA;AAGZC,oCAA4B,EAAE,IAHlB;AAIZC,YAAI,EAAE;AAJM,OAFT;AAQLC,aAAO,EAAE,EARJ;AASLC,cAAQ,EAAE;AATL,KAAP;AAWD,GApBY;;AAsBbC,SAAO,EAAE;AACPC,YAAQ,CAACC,IAAD,EAAO;AACb,WAAKC,OAAL,CAAaC,OAAb,CAAqBF,IAArB;AACD,KAHM;;AAKPG,WAAO,GAAG;AACRlB,WAAK,CAACmB,IAAN,CAAW,uCAAX,EACGC,IADH,CACQ,UAAUC,QAAV,EAAoB;AACxBC,eAAO,CAACC,GAAR,CAAYF,QAAZ;AACD,OAHH,EAIGG,KAJH,CAIS,UAAUC,KAAV,EAAiB;AACtBH,eAAO,CAACC,GAAR,CAAYE,KAAZ;AACD,OANH;AAOD;;AAbM,GAtBI;;AAsCbC,SAAO,GAAG;AACR,SAAKR,OAAL;AACD;;AAxCY,CAAf","names":["Swiper","SwiperSlide","Datepicker","ActivityCard","axios","require","default","components","data","selected","swiperOption","autoplay","pagination","aut