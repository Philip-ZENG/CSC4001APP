{"ast":null,"code":"const axios = require('axios').default;\n\nconst ACTIVITY_INFO_URL = 'http://localhost:4000/getActivityInfo';\nconst USER_ROLE = 'http://localhost:4000/getUserRole';\nexport default {\n  data() {\n    return {\n      activity_id: null,\n      activity_info: [],\n      user_role: ''\n    };\n  },\n\n  created() {\n    // get the route parameter 'id' from router\n    this.activity_id = this.$route.params.activity_id;\n  },\n\n  methods: {\n    getActivityInfo() {\n      axios.post(ACTIVITY_INFO_URL, {\n        activity_id: this.activity_id\n      }).then(response => {\n        this.activity_info.push(response.data);\n      });\n    },\n\n    getUserRole() {\n      axios.post(USER_ROLE, {\n        activity_id: this.activity_id,\n        user_id: this.user_id\n      }).then(response => {\n        this.activity_info.push(response.data);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,MAAMC,iBAAgB,GAAI,uCAA1B;AACA,MAAMC,SAAQ,GAAI,mCAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,IADR;AAELC,mBAAa,EAAE,EAFV;AAGLC,eAAS,EAAE;AAHN,KAAP;AAKD,GAPY;;AAQbC,SAAO,GAAG;AACR;AACA,SAAKH,WAAL,GAAmB,KAAKI,MAAL,CAAYC,MAAZ,CAAmBL,WAAtC;AACD,GAXY;;AAYbM,SAAO,EAAE;AACPC,mBAAe,GAAG;AAChBb,WAAK,CAACc,IAAN,CAAWX,iBAAX,EAA8B;AAAEG,mBAAW,EAAE,KAAKA;AAApB,OAA9B,EACGS,IADH,CACSC,QAAD,IAAc;AAClB,aAAKT,aAAL,CAAmBU,IAAnB,CAAwBD,QAAQ,CAACX,IAAjC;AACD,OAHH;AAID,KANM;;AAOPa,eAAW,GAAG;AACZlB,WAAK,CAACc,IAAN,CAAWV,SAAX,EAAsB;AAAEE,mBAAW,EAAE,KAAKA,WAApB;AAAiCa,eAAO,EAAE,KAAKA;AAA/C,OAAtB,EACGJ,IADH,CACSC,QAAD,IAAc;AAClB,aAAKT,aAAL,CAAmBU,IAAnB,CAAwBD,QAAQ,CAACX,IAAjC;AACD,OAHH;AAIF;;AAZO;AAZI,CAAf","names":["axios","require","default","ACTIVITY_INFO_URL","USER_ROLE","data","activity_id","activity_info","user_role","created","$route","params","methods","getActivityInfo","post","then","response","push","getUserRole","user_id"],"sourceRoot":"","sources":["D:\\MySQL\\pickup\\client\\src\\views\\ActivityManagementView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>This is the Activity Management page </h1>\n    <h2> The activity id is {{ activity_id }}</h2>\n  </div>\n</template>\n\n<script>\nconst axios = require('axios').default;\n\nconst ACTIVITY_INFO_URL = 'http://localhost:4000/getActivityInfo';\nconst USER_ROLE = 'http://localhost:4000/getUserRole';\n\nexport default {\n  data() {\n    return {\n      activity_id: null,\n      activity_info: [],\n      user_role: '',\n    };\n  },\n  created() {\n    // get the route parameter 'id' from router\n    this.activity_id = this.$route.params.activity_id;\n  },\n  methods: {\n    getActivityInfo() {\n      axios.post(ACTIVITY_INFO_URL, { activity_id: this.activity_id })\n        .then((response) => {\n          this.activity_info.push(response.data);\n        });\n    },\n    getUserRole() {\n      axios.post(USER_ROLE, { activity_id: this.activity_id, user_id: this.user_id })\n        .then((response) => {\n          this.activity_info.push(response.data);\n        });\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}