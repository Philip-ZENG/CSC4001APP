{"ast":null,"code":"const axios = require('axios').default;\n\nconst ACTIVITY_INFO_URL = 'http://localhost:4000/getActivityInfo';\nconst USER_ROLE = 'http://localhost:4000/getUserRole';\nexport default {\n  data() {\n    return {\n      activity_id: null,\n      user_id: null,\n      activity_info: [],\n      user_role: ''\n    };\n  },\n\n  created() {\n    // get the route parameter 'id' from router\n    this.activity_id = this.$route.params.activity_id;\n    this.user_id = this.$route.params.user_id;\n  },\n\n  methods: {\n    /**\n     * @description\n     * Return activity info of a specific activity with the provided activity_id\n     */\n    getActivityInfo() {\n      axios.post(ACTIVITY_INFO_URL, {\n        activity_id: this.activity_id\n      }).then(response => {\n        this.activity_info.push(response.data);\n      });\n    },\n\n    /**\n     * @description\n     * Return the role of a user with the provided activity_id and user_id\n     */\n    getUserRole() {\n      axios.post(USER_ROLE, {\n        activity_id: this.activity_id,\n        user_id: this.user_id\n      }).then(response => {\n        console.log(response);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAD,CAAP,CAAiBC,OAA/B;;AAEA,MAAMC,iBAAgB,GAAI,uCAA1B;AACA,MAAMC,SAAQ,GAAI,mCAAlB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,IADR;AAELC,aAAO,EAAE,IAFJ;AAGLC,mBAAa,EAAE,EAHV;AAILC,eAAS,EAAE;AAJN,KAAP;AAMD,GARY;;AASbC,SAAO,GAAG;AACR;AACA,SAAKJ,WAAL,GAAmB,KAAKK,MAAL,CAAYC,MAAZ,CAAmBN,WAAtC;AACA,SAAKC,OAAL,GAAe,KAAKI,MAAL,CAAYC,MAAZ,CAAmBL,OAAlC;AACD,GAbY;;AAcbM,SAAO,EAAE;AACP;;;;AAIAC,mBAAe,GAAG;AAChBd,WAAK,CAACe,IAAN,CAAWZ,iBAAX,EAA8B;AAAEG,mBAAW,EAAE,KAAKA;AAApB,OAA9B,EACGU,IADH,CACSC,QAAD,IAAc;AAClB,aAAKT,aAAL,CAAmBU,IAAnB,CAAwBD,QAAQ,CAACZ,IAAjC;AACD,OAHH;AAID,KAVM;;AAWP;;;;AAIAc,eAAW,GAAG;AACZnB,WAAK,CAACe,IAAN,CAAWX,SAAX,EAAsB;AAAEE,mBAAW,EAAE,KAAKA,WAApB;AAAiCC,eAAO,EAAE,KAAKA;AAA/C,OAAtB,EACGS,IADH,CACSC,QAAD,IAAc;AAClBG,eAAO,CAACC,GAAR,CAAYJ,QAAZ;AACD,OAHH;AAID;;AApBM;AAdI,CAAf","names":["axios","require","default","ACTIVITY_INFO_URL","USER_ROLE","data","activity_id","user_id","activity_info","user_role","created","$route","params","methods","getActivityInfo","post","then","response","push","getUserRole","console","log"],"sourceRoot":"","sources":["D:\\MySQL\\pickup\\client\\src\\views\\ActivityManagementView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>This is the Activity Management page </h1>\n    <h2> The activity id is {{ activity_id }}</h2>\n  </div>\n</template>\n\n<script>\nconst axios = require('axios').default;\n\nconst ACTIVITY_INFO_URL = 'http://localhost:4000/getActivityInfo';\nconst USER_ROLE = 'http://localhost:4000/getUserRole';\n\nexport default {\n  data() {\n    return {\n      activity_id: null,\n      user_id: null,\n      activity_info: [],\n      user_role: '',\n    };\n  },\n  created() {\n    // get the route parameter 'id' from router\n    this.activity_id = this.$route.params.activity_id;\n    this.user_id = this.$route.params.user_id;\n  },\n  methods: {\n    /**\n     * @description\n     * Return activity info of a specific activity with the provided activity_id\n     */\n    getActivityInfo() {\n      axios.post(ACTIVITY_INFO_URL, { activity_id: this.activity_id })\n        .then((response) => {\n          this.activity_info.push(response.data);\n        });\n    },\n    /**\n     * @description\n     * Return the role of a user with the provided activity_id and user_id\n     */\n    getUserRole() {\n      axios.post(USER_ROLE, { activity_id: this.activity_id, user_id: this.user_id })\n        .then((response) => {\n          console.log(response);\n        });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}